{
  "attr-bans": [
    "bgcolor",
    "dynsrc",
    "id",
    "frameborder",
    "longdesc",
    "lowsrc",
    "onclick",
    "ondblclick",
    "onload",
    "marginwidth",
    "marginheight",
    "scrolling"
  ],
  "attr-name-ignore-regex": false,
  "attr-name-style": false,
  "attr-no-dup": true,
  "attr-no-unsafe-chars": false,
  "attr-no-unsafe-char": false,
  "attr-quote-style": "double",
  "attr-req-value": false,
  "class-no-dup": true,
  "class-style": "dash",
  "csslint": false,
  "doctype-first": false,
  "doctype-html5": false,
  "fig-req-figcaption": true,
  "focusable-tabindex-style": false,
  "head-req-title": true,
  "href-style": false,
  "html-req-lang": true,
  "id-class-ignore-regex": false,
  "id-class-no-ad": true,
  "id-class-style": "underscore",
  "id-no-dup": true,
  "input-radio-req-name": true,
  "img-req-alt": false,
  "img-req-src": false,
  "indent-style": "spaces",
  "indent-width": 2,
  "indent-width-cont": true,
  "input-req-label": false,
  "label-req-for": true,
  "lang-style": "case",
  "line-end-style": false,
  "line-max-len": 12000,
  "line-max-len-ignore-regex": "/href/g",
  "page-title": true,
  "spec-char-escape": false,
  "table-req-caption": false,
  "table-req-header": true,
  "tag-bans": [
    "keygen"
  ],
  "tag-close": false,
  "tag-name-lowercase": true,
  "tag-name-match": true,
  "tag-self-close": false,
  "text-escape-spec-char": true,
  "title-max-len": 60,
  "title-no-dup": true
}
